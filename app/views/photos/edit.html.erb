<%= render "shared/header" %>
<%= render "shared/second-header" %>

<h3>写真館　編集</h3>
<p>写真や説明文を編集してください</p>
<%= form_with model: @photo, local: true do |form| %>
  <div class="image">
    <div>
      クリックして写真を選ぶ
    </div>
      <%# form.file_field :image %>
  </div>
  <div class="text">
    <%= form.label :photo_text, "説明文" %>
    <%= form.text_area :photo_text, rows: "7" %>
  </div>
  <div class="submit">
    <%= form.submit "編集する" %>
  </div>
<% end %>